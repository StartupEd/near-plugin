                <div class="symbol symbol-85 text-center mr-10" @onclick="@(()=>BorderColorChange(4))">
                    @if (IsIconActive_4)
                    {
                        <span class="symbol-label">
                            <img src="_content/OneUX/assets/imgs/welcome/Intrapreneur.svg" />
                        </span>
                    }
