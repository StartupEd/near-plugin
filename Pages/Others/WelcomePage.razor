                <div class="symbol symbol-85 text-center mr-10" @onclick="@(()=>BorderColorChange(6))">
                    @if (IsIconActive_6)
                    {
                        <span class="symbol-label"><img src="_content/OneUX/assets/imgs/welcome/Incubator.svg" /></span>
                    }
                    else
                    {
                        <span class="symbol-label"><img src="_content/OneUX/assets/imgs/welcome/Incubator_d.svg" /></span>
                    }
                    <span>Incubator</span>
                </div>
                <div class="symbol symbol-85 text-center mr-10" @onclick="@(()=>BorderColorChange(7))">
                    @if (IsIconActive_7)
                    {
                        <span class="symbol-label">
                            <img src="_content/OneUX/assets/imgs/welcome/StudentEnterpreneur.svg" />
                        </span>
                    }
                    else
                    {
                        <span class="symbol-label"><img src="_content/OneUX/assets/imgs/welcome/StudentEnterpreneur_d.svg" /></span>

                    }
                    <span>Student <br />Entrepreneur</span>
                </div>
                <div class="symbol symbol-85 text-center" @onclick="@(()=>BorderColorChange(8))">
                    @if (IsIconActive_8)
                    {
                        <span class="symbol-label"><img src="_content/OneUX/assets/imgs/welcome/Others.svg" /></span>
                    }
                    else
                    {
                        <span class="symbol-label"><img src="_content/OneUX/assets/imgs/welcome/Other_d.svg" /></span>
                    }
                    <span>Other</span>
                </div>
            </div>
            <p style="margin-top: 10px;">Not as per your profession? Don't worry you can mention below :</p>
            @if (IsIconActive_8)
            {
                <input @bind="OtherPersonaName" class="form-control form-control-solid form-control-lg" id="personaId" placeholder="Eg. Freelancer" autocomplete="off">
            }
            else
            {
                <input class="form-control form-control-solid form-control-lg" id="personaId" placeholder="Eg. Freelancer" autocomplete="off" disabled>
            }
        </div>
    }

