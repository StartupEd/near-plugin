<div class="message-box">

    <h2>
        Welcome to StartupEd!
    </h2>
    <!--begin: Wizard Step 1-->
    @if (IsFirstStep == true)
    {
        <div class="wizard-step" data-wizard-type="step-content" data-wizard-state="current">
            <h5>
                how do you plan to make use of Incubation Cloud:
            </h5>
            <div class="symbol-list d-flex flex-wrap">
                <div class="symbol symbol-100 text-center mr-15 " @onclick="@(()=>BorderColorChange(17))">
                    @if (IsIconActive_17)
                    {
                        <span class="symbol-label @BorderColor">Networking</span>
                    }
                    else
                    {
                        <span class="symbol-label">Networking</span>
                    }
                    
                </div>
                <div class="symbol symbol-100 text-center mr-15" @onclick="@(()=>BorderColorChange(18))">
                    @if (IsIconActive_18)
                    {
                        <span class="symbol-label @BorderColor">Business Management</span>
                    }
                    else
                    {
                        <span class="symbol-label">Business Management</span>
                    }
                    
                </div>
                <div class="symbol symbol-100 text-center mr-15" @onclick="@(()=>BorderColorChange(19))">
                    @if (IsIconActive_19)
                    {
                        <span class="symbol-label @BorderColor">Business Development</span>
                    }
                    else
                    {
                        <span class="symbol-label">Business Development</span>
                    }
                   
                </div>
                <div class="symbol symbol-100 text-center mr-15 mt-10" @onclick="@(()=>BorderColorChange(20))">
                    @if (IsIconActive_20)
                    {
                        <span class="symbol-label @BorderColor">Mentorship</span>
                    }
                    else
                    {
                        <span class="symbol-label">Mentorship</span>
                    }
                </div>
                <div class="symbol symbol-100 text-center mr-15 mt-10" @onclick="@(()=>BorderColorChange(21))">
                    @if (IsIconActive_21)
                    {
                        <span class="symbol-label @BorderColor">Scouting</span>
                    }
                    else
                    {
                         <span class="symbol-label">Scouting</span>
                    }
                   
                </div>
                <div class="symbol symbol-100 text-center mt-10" @onclick="@(()=>BorderColorChange(22))">
                    @if (IsIconActive_22)
                    {
                        <span class="symbol-label @BorderColor">Other</span>
                    }
                    else
                    {
                        <span class="symbol-label">Other</span>
                    }
                </div>
            </div>
            <p style="margin-top: 10px;">Not as per your choice? Don't worry you can mention below :</p>
            @if (IsIconActive_22)
            {
                <input @bind="OtherIcPlan" class="form-control form-control-solid form-control-lg" id="personaId" placeholder="Eg. Freelancer" autocomplete="off">
            }
            else
            {
                <input class="form-control form-control-solid form-control-lg" id="personaId" placeholder="Eg. xyz" autocomplete="off" disabled>
            }
        </div>
    }
