                </div>
                <div class="symbol symbol-85 text-center mr-10" @onclick="@(()=>BorderColorChange(15))">
                    @if (IsIconActive_15)
                    {
                        <span class="symbol-label"><img src="_content/OneUX/assets/imgs/welcome/Growth.svg" /></span>
                    }
                    else
                    {
                        <span class="symbol-label"><img src="_content/OneUX/assets/imgs/welcome/Growth_d.svg" /></span>
                    }
                    <span>Growth</span>
                </div>
                <div class="symbol symbol-85 text-center" @onclick="@(()=>BorderColorChange(16))">
                    @if (IsIconActive_16)
                    {
                        <span class="symbol-label"><img src="_content/OneUX/assets/imgs/welcome/Others.svg" /></span>
                    }
                    else
                    {
                        <span class="symbol-label"><img src="_content/OneUX/assets/imgs/welcome/Other_d.svg" /></span>
                    }
                    <span>Other</span>
                </div>
            </div>
            <p style="margin-top: 10px;">Not as per your choice? Don't worry you can mention below :</p>
            @if (IsIconActive_16)
            {
                <input @bind="OtherProductName" class="form-control form-control-solid form-control-lg" id="ProductId" placeholder="Eg. xyz" autocomplete="off">
            }
            else
            {
                <input class="form-control form-control-solid form-control-lg" id="ProductId" placeholder="Eg. xyz" autocomplete="off" disabled>
            }

        </div>
    }

    <!--end: Wizard Step 2-->
    <div class="d-flex mt-5">
        <ul class="nav" style="margin-left:230px;">
            @if (IsFirstStep)
            {
                <li class="navi-item"><span class="dot"></span></li>
            }
            else
            {
                <li class="navi-item" style="opacity:@Opacity"><span class="dot"></span></li>
            }
            @if (IsSecondStep)
            {
                <li class="navi-item"><span class="dot"></span></li>
            }
            else
            {
                <li class="navi-item" style="opacity:@Opacity"><span class="dot"></span></li>
            }
            @if (IsThirdStep)
            {
                <li class="navi-item"><span class="dot"></span></li>
            }
            else
            {
                <li class="navi-item" style="opacity:@Opacity"><span class="dot"></span></li>
            }
        </ul>
    </div>



    <div class="d-flex justify-content-between mt-0">
        <div class="mr-2">
            @if (IsSecondStep)
            {
                <button type="button" class="btn btn-primary font-weight-bold text-uppercase px-9 py-4" data-wizard-type="action-next" id="nextTab" @onclick="PreviousStep">
                    Previous
                </button>
            }
            else if (IsThirdStep)
            {
                <button type="button" class="btn btn-primary font-weight-bold text-uppercase px-9 py-4" data-wizard-type="action-next" id="nextTab" @onclick="PreviousStep">
                    Previous
                </button>
            }
            else
            {
                <button type="button" class="btn btn-primary font-weight-bold text-uppercase px-9 py-4" data-wizard-type="action-next" id="nextTab" disabled>
                    Previous
                </button>
            }

        </div>
        <div>
            @if (IsThirdStep)
            {
                <button type="button" class="btn btn-primary font-weight-bold text-uppercase px-9 py-4" data-wizard-type="action-next" id="nextTab" @onclick="HandleClick">
