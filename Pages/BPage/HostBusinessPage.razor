                                                        </div>
                                                        <!--end: Wizard Step 1-->
                                                        <!--begin: Wizard Step 3-->
                                                        <div class="pb-5" data-wizard-type="step-content">
                                                            <!--begin::Title-->
                                                            <div class="pb-10 pb-lg-15">
                                                                <h3 class="font-weight-bolder text-dark font-size-h2">Page Details</h3>
                                                            </div>
                                                            <OneUX_InputBox @bind-Value="Model.Name" Label="Name" ElementId="pageName" PlaceholderText="Enter name of the page" StarValidation="@Validation" />
                                                            <OneUX_ImageInputBox ReturnImgbase64="Imagebase64" ImageDataUrl="@Model.Core.ImageUrl" Label="CoverImage" />

                                                            <OneUX_InputSelect @bind-Value="Model.FocusIndustry" Label="Focus Industry" Id="industryList" Title="Select Industry" StarValidation="@Validation">
                                                                @foreach (var industry in FocusIndustry)
                                                                {
                                                                    <option value=@industry>@industry</option>
                                                                }
                                                            </OneUX_InputSelect>
                                                            @if (Model.Category == "INCUBATOR")
                                                            {
                                                                <OneUX_InputSelect @bind-Value="Model.IncubatorDetails.Type" Label="Incubator Type" Id="incubatorList" Title="Select Type" StarValidation="@Validation">
                                                                    @foreach (var incubatorType in Enum.GetValues(typeof(IncubatorType)))
                                                                        {
                                                                        <option value="@incubatorType.ToString()">@incubatorType.ToString()</option>
                                                                        }
                                                                </OneUX_InputSelect>

                                                                <OneUX_MultiSelect ElementId="incubationServices" Label="Incubation Services" PlaceholderText="Select Services" MultipleValues="multiple">
                                                                    @foreach (var service in IncubationServiceList)
                                                                    {
                                                                        <option value="@service">@service</option>
                                                                    }
                                                                </OneUX_MultiSelect>
                                                            }
                                                            else if (Model.Category == "E-CELL")
                                                            {
                                                                <OneUX_InputBox @bind-Value="Model.ECellDetails.UniversityName" Label="University Name" PlaceholderText="University Associated with the ECell" StarValidation="@Validation" />
                                                                <OneUX_InputBox @bind-Value="Model.ECellDetails.NoOfStartupsAssociated" Label="Startups Associated" PlaceholderText="Total No of Startups Associated with the ECell" StarValidation="@Validation" />
                                                            }
                                                            else if (Model.Category == "STARTUP")
                                                            {
                                                                <OneUX_InputSelect @bind-Value="Model.StartupDetails.StartupStage" Label="Startup Stage" Id="stageList" Title="Select Stage" StarValidation="@Validation">
                                                                    @foreach (var stage in StartupStage)
                                                                        {
                                                                        <option value=@stage>@stage</option>
                                                                        }
                                                                </OneUX_InputSelect>
                                                            }
                                                            <OneUX_QuillEditor ElementId="businessPageAbout" PlaceholderText="Description" Label="Description" Summary="@Model.Core.Description" StarValidation="@Validation" />
                                                        </div>
                                                        <!--end: Wizard Step 2-->
                                                        <!--begin: Wizard Step 3-->
                                                        <div class="pb-5" data-wizard-type="step-content">
                                                            <h4 class="mb-10 font-weight-bold text-dark">Enter Page Location</h4>
                                                            <OneUX_GetLocation ReturnLocation="GetLocation" />

                                                            @if (!string.IsNullOrEmpty(Model.Address.Country))
                                                            {
                                                                <OneUX_InputSelect @bind-Value="Model.Address.Country" Label="Country" Id="countryInputSelect" Title="Select Country" StarValidation="@Validation">
                                                                    <option selected="selected" value=@Model.Address.Country>@Model.Address.Country</option>
                                                                    @foreach (var country in CountryList)
                                                                        {
                                                                        <option value=@country>@country</option>

                                                                        }
                                                                </OneUX_InputSelect>
                                                            }
                                                            else
                                                            {
                                                                <OneUX_InputSelect @bind-Value="Model.Address.Country" Label="Country" Id="countryInputSelect" Title="Select Country" StarValidation="@Validation">
                                                                    @foreach (var country in CountryList)
                                                                        {
                                                                        <option value=@country>@country</option>
                                                                        }
                                                                </OneUX_InputSelect>
                                                            }

                                                            <OneUX_InputBox @bind-Value="Model.Address.City" Label="City" PlaceholderText="City" ElementId="cityInputId" SpanElement="Please enter your City." />
                                                            <OneUX_InputBox @bind-Value="Model.Address.State" Label="State" PlaceholderText="State" ElementId="stateInputId" SpanElement="Please enter your State." />
                                                            <OneUX_InputBox @bind-Value="Model.Address.Zip" Label="Postal Code" PlaceholderText="Zip Code" ElementId="zipInputId" SpanElement="Please enter your Postcode." />
                                                        </div>
                                                        <!--end: Wizard Step 3-->
                                                        <!--begin: Wizard Step 4-->
                                                        <div class="pb-5" data-wizard-type="step-content">
                                                            <h4 class="mb-10 font-weight-bold text-dark">Enter Contact Details</h4>
                                                            <OneUX_InputBox @bind-Value="Model.ContactInfo.Name" ElementId="contactName" PlaceholderText="Name" Label="Contact Name" StarValidation="@Validation" />
                                                            <OneUX_InputBox @bind-Value="Model.ContactInfo.PhoneNo" ElementId="phone" PlaceholderText="Phone" Label="Contact Phone" MaxLength="10" StarValidation="@Validation" />
                                                            <OneUX_InputBox @bind-Value="Model.ContactInfo.Email" ElementId="email" PlaceholderText="Email" Label="Email Address" StarValidation="@Validation" />
                                                        </div>
                                                        <!--end: Wizard Step 4-->
                                                        <!--begin: Wizard Step 5-->
                                                        <div class="pb-5" data-wizard-type="step-content">
                                                            <h4 class="mb-10 font-weight-bold text-dark">Review your Details and Submit</h4>
                                                            <!--begin::Section-->
                                                            <h6 class="font-weight-bold mb-3">
                                                                Page Details:
                                                            </h6>
                                                            <table class="w-100">
                                                                <tr>
                                                                    <td class="font-weight-bold text-muted">Page Name:</td>
                                                                    <td class="font-weight-bold text-right">@Model.Name</td>
                                                                </tr>
                                                                <tr>
                                                                    <td class="font-weight-bold text-muted">Page Category:</td>
                                                                    <td class="font-weight-bold text-right">@Model.Category</td>
                                                                </tr>
                                                                <tr>
                                                                    <td class="font-weight-bold text-muted">Focus Industry:</td>
                                                                    <td class="font-weight-bold text-right">@Model.FocusIndustry</td>
                                                                </tr>
                                                            </table>
                                                            <div class="separator separator-dashed my-5"></div>

                                                            <h6 class="font-weight-bold mb-3">
                                                                Contact Info:
                                                            </h6>
                                                            <table class="w-100">
                                                                <tr>
                                                                    <td class="font-weight-bold text-muted">Name:</td>
                                                                    <td class="font-weight-bold text-right">@Model.ContactInfo.Name</td>
                                                                </tr>
                                                                <tr>
                                                                    <td class="font-weight-bold text-muted">Phone:</td>
                                                                    <td class="font-weight-bold text-right">@Model.ContactInfo.PhoneNo</td>
                                                                </tr>
                                                                <tr>
                                                                    <td class="font-weight-bold text-muted">Email:</td>
                                                                    <td class="font-weight-bold text-right">@Model.ContactInfo.Email</td>
                                                                </tr>
                                                            </table>
                                                            <div class="separator separator-dashed my-5"></div>
                                                        </div>
                                                        <!--end: Wizard Step 5-->

                                                    </CascadingValue>
                                                </CascadingValue>
                                            </CascadingValue>
                                        </CascadingValue>

                                    </div>
                                    <!--end: Wizard-->
                                </div>
                            </div>
                            <!--end: Wizard Form-->
                            <!--begin: Wizard Actions-->
                            <div class="d-flex justify-content-between pt-3">
                                <div class="mr-2">
                                    <button type="button" class="btn btn-light-primary font-weight-bolder font-size-h6 pl-6 pr-8 py-4 my-3 mr-3" data-wizard-type="action-prev">
                                        <span class="svg-icon svg-icon-md mr-1">
